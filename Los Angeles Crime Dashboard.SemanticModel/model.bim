        "lineageTag": "bfe11696-b6dc-48e9-935f-10ca231db947",
        "partitions": [
          {
            "name": "LA Crime Data",
            "mode": "import",
            "source": {
              "expression": [
                "let",
                "    Source = OData.Feed(\"https://data.lacity.org/api/odata/v4/2nrs-mtv8\", null, [Implementation=\"2.0\"]),",
                "    #\"Changed Type\" = Table.TransformColumnTypes(Source,{{\"date_rptd\", type datetime}, {\"date_occ\", type datetime}, {\"crm_cd\", Int64.Type}, {\"vict_age\", Int64.Type}}),",
                "    #\"Remove zero lat\" = Table.SelectRows(#\"Changed Type\", each [lat] <> 0),",
                "    #\"Remove zero lon\" = Table.SelectRows(#\"Remove zero lat\", each [lon] <> 0),",
                "    #\"Add UCR type 1\" = Table.AddColumn(#\"Remove zero lon\", \"UCR Reporting\", each if List.ContainsAny({[crm_cd]}, {110, 113}) then \"HOMICIDE\"",
                "else if List.ContainsAny({[crm_cd]}, {121, 122, 815, 820, 821}) then \"RAPE\"",
                "else if List.ContainsAny({[crm_cd]}, {210, 220}) then \"ROBBERY\"",
                "else if List.ContainsAny({[crm_cd]}, {230, 231, 235, 236, 250,251, 761, 926}) then \"AGG. ASSAULTS\"",
                "else if List.ContainsAny({[crm_cd]}, {435, 436, 437, 622, 623, 624, 625, 626, 627, 647, 763, 928, 930}) then \"SIMPLE ASSAULT\"",
                "else if List.ContainsAny({[crm_cd]}, {310, 320}) then \"BURGLARY\"",
                "else if List.ContainsAny({[crm_cd]}, {510, 520, 433}) then \"MVT (GTA)\"",
                "else if List.ContainsAny({[crm_cd]}, {330, 331, 410, 420, 421}) then \"BTFV\"",
                "else if List.ContainsAny({[crm_cd]}, {350, 351, 352, 353, 450, 451, 452, 453}) then \"PERSONAL THEFT\"",
                "else if List.ContainsAny({[crm_cd]}, {341, 343, 345, 440, 441, 442, 443, 444, 445, 470, 471, 472, 473, 474, 475, 480, 485, 487, 491}) then \"OTHER THEFT\"",
                "else \"PART II OFFENSE\"),",
                "    #\"Calculate days to report\" = Table.AddColumn(#\"Add UCR type 1\", \"Days to Report\", each [date_rptd] - [date_occ]),",
                "    #\"Convert to days\" = Table.TransformColumns(#\"Calculate days to report\",{{\"Days to Report\", Duration.Days, Int64.Type}}),",
                "    #\"Expand descendant\" = Table.AddColumn(#\"Convert to days\", \"Victim Descent\", each if [vict_descent] = \"A\" then \"Other Asian\" else if [vict_descent] = \"B\" then \"Black\" else if [vict_descent] = \"C\" then \"Chinese\" else if [vict_descent] = \"D\" then \"Cambodian\" else if [vict_descent] = \"F\" then \"Filipino\" else if [vict_descent] = \"G\" then \"Guamaian\" else if [vict_descent] = \"H\" then \"Hispanic/Latin/Mexican\" else if [vict_descent] = \"I\" then \"American Indian/Alaskan Native\" else if [vict_descent] = \"J\" then \"Japanese\" else if [vict_descent] = \"K\" then \"Korean\" else if [vict_descent] = \"L\" then \"Laotian\" else if [vict_descent] = \"O\" then \"Other\" else if [vict_descent] = \"P\" then \"Pacific Islander\" else if [vict_descent] = \"S\" then \"Samoan\" else if [vict_descent] = \"U\" then \"Hawaiian\" else if [vict_descent] = \"V\" then \"Vietnamese\" else if [vict_descent] = \"W\" then \"White\" else if [vict_descent] = \"X\" then \"Unknown\" else if [vict_descent] = \"Z\" then \"Asian Indian\" else \"Unknown\"),",
                "    #\"Clean location name\" = Table.AddColumn(#\"Expand descendant\", \"Location Name\", each Text.Combine(List.Select(Text.SplitAny([location], \" #(lf)#(cr)\"), each _<>\"\"), \" \")),",
                "    #\"Removed original columns\" = Table.RemoveColumns(#\"Clean location name\",{\"vict_descent\", \"location\"}),",
                "    #\"Renamed Columns\" = Table.RenameColumns(#\"Removed original columns\",{{\"area_name\", \"Area Name\"}, {\"dr_no\", \"Record Number\"}, {\"vict_age\", \"Victim Age\"}, {\"weapon_desc\", \"Weapon Type\"}})",
                "in",
                "    #\"Renamed Columns\""
              ],
              "type": "m"
            }
          }
        ]
