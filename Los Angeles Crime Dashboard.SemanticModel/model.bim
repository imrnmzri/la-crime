          },
          {
            "name": "Crime Type with The Most Crime Recorded",
            "expression": [
              "",
              "",
              "VAR summary_table =",
              "SUMMARIZE(",
              "    'LA Crime Data',",
              "    'LA Crime Data'[UCR Reporting],",
              "    \"Total Crime Recorded\", DISTINCTCOUNT('LA Crime Data'[Record Number])",
              ")",
              "",
              "VAR max_value =",
              "MAXX(",
              "    summary_table,",
              "    [Total Crime Recorded]",
              ")",
              "",
              "RETURN",
              "CALCULATE(",
              "    SELECTEDVALUE('LA Crime Data'[UCR Reporting]),",
              "    FILTER(summary_table, [Total Crime Recorded] = max_value)",
              ")"
            ],
            "lineageTag": "2ebc31c4-7424-4e24-9fb7-3319896b8782"
